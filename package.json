{
  "name": "quizdom",
  "version": "1.0.0",
  "description": "A quiz application for creating and taking quizzes",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "start": "cd backend && npm run start:prod",
    "build": "cd frontend && npm install && npm run build && cd ../backend && npm install && npm run build",
    "dev:frontend": "cd frontend && npm start",
    "dev:backend": "cd backend && npm start",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\""
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "quiz",
    "education",
    "assessment"
  ],
  "author": "",
  "bugs": {
